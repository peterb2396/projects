#lang scheme
(define (abs-each nums)
  (if (null? nums) 
      (list)
      (cons (abs(car nums)) (abs-each (cdr nums)))))
(abs-each (list -1.1 -2 3))
(abs-each (list 4.3 43))
(abs-each (list))
(abs-each (list -18.5))
(abs-each (list 1 -2 3 -4 5 -6))
(abs-each (list -78 -20))
(abs-each (list -4))
(abs-each (list -3 -2.4 -2 -32 0))
(abs-each (list 0 1 2 -3))
(abs-each (list 65 -82.91 -18 -9))